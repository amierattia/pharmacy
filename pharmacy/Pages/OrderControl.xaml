<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="pharmacy.Pages.OrderControl"
             Shell.NavBarIsVisible="False"
             Title="OrderControl">
    <VerticalStackLayout Padding="20" Spacing="10">
        <Frame BackgroundColor="#f3f6fb">
            <Entry Keyboard="Numeric" x:Name="Id" Placeholder="Enter Id" Background="#f3f6fb"/>
        </Frame>
        
        
        <Button Text="Edit" x:Name="EditeD" Clicked="EditeD_Clicked"/>
        <Button Text="Delet" x:Name="Delet" Clicked="Delet_Clicked"/>
        <ScrollView x:Name="Uodate">
            
        </ScrollView>
    </VerticalStackLayout>
</ContentPage>
    